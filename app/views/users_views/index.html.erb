<p id="notice"><%= notice %></p>

<h1>Users Views</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_views.each do |users_view| %>
      <tr>
        <td><%= link_to 'Show', users_view %></td>
        <td><%= link_to 'Edit', edit_users_view_path(users_view) %></td>
        <td><%= link_to 'Destroy', users_view, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users View', new_users_view_path %>
